{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env":{
        "browser": true,
        "node": true
    },
    "rules": {
        "max-len": [2, 120],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "indent": [2, 4, { "SwitchCase": 1, "VariableDeclarator": 1, "outerIIFEBody": 1 }]
    }
}
